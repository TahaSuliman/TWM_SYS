@model IEnumerable<Curly_TWM.Domain.Entities.Teams>

@{
    ViewBag.Title = "Main";
    Layout = "~/Views/Initiative_Officer/_LayoutInitiativeOfficer.cshtml";
}

<script>
    "use strict";

    //1

    var KTProjectList = {

        init: function () {
            !function () {

                //MyData
    var t =@ViewBag.InitsCou;
    var p =@ViewBag.InitsPlanCou ;
    var e =@ViewBag.InitsRunCou;
    var d =@ViewBag.InitsDoneCou;

                let plan = p / t * 100;
                let exut = e / t * 100;
                let donee = d / t * 100;

                var t = document.getElementById("kt_project_list_chart");
                if (t) {
                    var e = t.getContext("2d");
                    new Chart(e, {
                        type: "doughnut",
                        data: {
                            datasets: [{
                                data: [Math.round(exut), Math.round(donee), Math.round(plan) ],
                                backgroundColor: ["#00A3FF", "#50CD89", "#E4E6EF"]
                            }],
                            labels: ["قيد التنفيذ", "تم الانجاز", "قيد التخطيط"]
                        },
                        options: {
                            chart: { fontFamily: "inherit" },
                            cutout: "75%",
                            cutoutPercentage: 65,
                            responsive: !0,
                            maintainAspectRatio: !1,
                            title: { display: !1 },
                            animation: { animateScale: !0, animateRotate: !0 },
                            tooltips: {
                                enabled: !0,
                                intersect: !1,
                                mode: "nearest",
                                bodySpacing: 5,
                                yPadding: 10,
                                xPadding: 10,
                                caretPadding: 0,
                                displayColors: !1,
                                backgroundColor: "#20D489",
                                titleFontColor: "#ffffff",
                                cornerRadius: 4,
                                footerSpacing: 0,
                                titleSpacing: 0
                            }, plugins: { legend: { display: !1 } }
                        }
                    })
                }
            }()
        }
    };


    KTUtil.onDOMContentLoaded((function () {
        KTProjectList.init()
    }));


</script>

<script>
    "use strict";
    //2
    var taskschart = {

        init: function () {
            !function () {

                //MyData
                var t =@ViewBag.taskCou;
                var n =@ViewBag.NtaskCou;
                var e =@ViewBag.EtaskCou;
                var d =@ViewBag.DtaskCou;
                var o =@ViewBag.OtaskCou;

                let plan = n / t * 100;
                let exut = e / t * 100;
                let donee = d / t * 100;
                let outa = o / t * 100;

                var t = document.getElementById("taskscript_chart");
                if (t) {
                    var e = t.getContext("2d");
                    new Chart(e, {
                        type: "doughnut",
                        data: {
                            datasets: [{
                                data: [Math.round(exut), Math.round(donee), Math.round(plan), Math.round(outa)],
                                backgroundColor: ["#00A3FF", "#50CD89", "#E4E6EF", "red"]
                            }],
                            labels: ["قيد التنفيذ", "تم التنفيذ", "جديد", "خارج الوقت"]
                        },
                        options: {
                            chart: { fontFamily: "inherit" },
                            cutout: "75%",
                            cutoutPercentage: 65,
                            responsive: !0,
                            maintainAspectRatio: !1,
                            title: { display: !1 },
                            animation: { animateScale: !0, animateRotate: !0 },
                            tooltips: {
                                enabled: !0,
                                intersect: !1,
                                mode: "nearest",
                                bodySpacing: 7,
                                yPadding: 10,
                                xPadding: 10,
                                caretPadding: 0,
                                displayColors: !1,
                                backgroundColor: "#20D489",
                                titleFontColor: "#ffffff",
                                cornerRadius: 4,
                                footerSpacing: 0,
                                titleSpacing: 0
                            }, plugins: { legend: { display: !1 } }
                        }
                    })
                }
            }()
        }
    };
    KTUtil.onDOMContentLoaded((function () {
        taskschart.init()
    }
    ));
</script>
